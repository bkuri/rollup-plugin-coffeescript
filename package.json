{
  "author": "Bernardo Kuri",
  "browser": "dist/rollup-plugin-coffeescript.umd.js",
  "description": "Modern CoffeeScript support for Rollup.",
  "homepage": "https://github.com/bkuri/rollup-plugin-coffeescript",
  "license": "MIT",
  "main": "dist/rollup-plugin-coffeescript.cjs",
  "module": "dist/rollup-plugin-coffeescript.mjs",
  "name": "@bkuri/rollup-plugin-coffeescript",
  "type": "module",
  "version": "0.3.4",
  "bugs": {
    "url": "https://github.com/bkuri/rollup-plugin-coffeescript/issues"
  },
  "dependencies": {
    "coffeescript": "^2.7.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/pluginutils": "^5.0.4",
    "del-cli": "^5.1.0",
    "mocha": "^10.2.0",
    "rollup": "^3.29.2",
    "rollup-plugin-polyfill-node": "^0.12.0"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "coffeescript",
    "rollup-plugin",
    "vite-plugin"
  ],
  "peerDependencies": {
    "coffeescript": "2.7.0",
    "rollup": "^3.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bkuri/rollup-plugin-coffeescript.git"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "del-cli dist",
    "ci:coverage": "nyc pnpm test && nyc report --reporter=text-lcov > coverage.lcov",
    "ci:lint": "pnpm build && pnpm lint",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "pnpm test -- --verbose",
    "prebuild": "pnpm clean",
    "prepare": "pnpm build",
    "prerelease": "pnpm build",
    "pretest": "pnpm build",
    "release": "pnpm plugin:release --workspace-root -- --pkg $npm_package_name",
    "test": "mocha"
  }
}
